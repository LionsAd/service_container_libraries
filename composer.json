  "description": "Service Container Libraries",
  "type": "drupal-module",
  "homepage": "https://www.drupal.org/project/service_container",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    { "name": "Fabian Franz", "email": "fabian@lionsad.de" }
  ],
  "require": {
    "php": ">=5.3.10"
